{
    "name": "steamscore/steamscore",
    "description": "SteamScore JSON API",
    "type": "project",
    "homepage": "https://steamscore.info",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jonas@stendahl.me"
        }
    ],
    "support": {
       "issues": "https://github.com/jyggen/SteamScore/issues",
       "source": "https://github.com/jyggen/SteamScore"
    },
    "require": {
        "php": "^7.0",
        "filp/whoops": "^2.0",
        "zendframework/zend-expressive": "^1.0",
        "zendframework/zend-expressive-helpers": "^2.0",
        "zendframework/zend-expressive-zendrouter": "^1.0",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-stdlib": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^v2.0.0-alpha",
        "jakub-onderka/php-parallel-lint": "^0.9",
        "phpro/grumphp": "^0.9",
        "phpunit/phpunit": "^5.0",
        "sensiolabs/security-checker": "^4.0",
        "squizlabs/php_codesniffer": "^3.0.0RC1"

    },
    "autoload": {
        "psr-4": {
            "SteamScore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SteamScore\\Tests\\": "test/"
        }
    },
    "config": {
        "platform": {
            "php": "7.0"
        },
        "preferred-install": "dist"
    },
    "scripts": {
    },
    "bin": [
        "bin/console"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}

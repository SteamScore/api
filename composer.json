{
    "name": "steamscore/steamscore",
    "description": "Zend expressive skeleton. Begin developing PSR-7 middleware applications in seconds",
    "type": "project",
    "homepage": "https://steamscore.info",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jonas@stendahl.me"
        }
